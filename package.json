{
  "name": "pixri-css",
  "version": "1.1.11",
  "description": "A lightweight beginner-friendly CSS framework like Tailwind.",
  "main": "dist/pixricss.min.css",
  "style": "dist/pixricss.min.css",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "mkdir -p dist && cp src/pixricss.css dist/pixricss.css",
    "build:min": "postcss src/pixricss.css -o dist/pixricss.min.css",
    "prepare": "npm run build"
  },
  "keywords": [
  "css-framework",
  "utility-first",
  "tailwind-inspired",
  "responsive-ui",
  "lightweight-css",
  "frontend",
  "cdn-support",
  "beginner-friendly",
  "grid-system",
  "typography",
  "buttons",
  "pixri-css"
],
  "author": "SHRI",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1"
  }
}
